<div class="col-md-3 sidebar">
    <h4>메뉴 리스트</h4>
    <div class="list-group">
        <% if (menuList && menuList.length > 0) { %>
            <% menuList.forEach(menu => { %>
                <a href="<%= menu.link %>" class="list-group-item list-group-item-action">
                    <%= menu.name %>
                </a>
            <% }) %>
        <% } else { %> 
            <p class="list-group-item">메뉴 없음</p>
        <% } %>
    </div>
</div>
